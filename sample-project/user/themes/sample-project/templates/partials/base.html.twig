<!DOCTYPE html>
<html lang="en">
  <head>
  {% set theme_config = attribute(config.themes, config.system.pages.theme) %}

  {% block head %}
      <meta charset="utf-8" />
      <title>
        {% if header.title %}{{ header.title|e('html') }} | {% endif %}
        {{ site.title|e('html') }}
      </title>
      {% include 'partials/metadata.html.twig' %}
      <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
      <meta property="og:url" content="{{ base_url_absolute }}{{ page.url }}" />
      <meta property="og:type" content="website" />
      <meta property="og:title" content="{{page.title}}" />
      <meta property="fb:app_id" content="731878800318903" />
      <meta property="og:image" content="{{ url('theme://images/webbanner.png', true) }}" />
      <link rel="icon" type="image/ico" href="{{ url('theme://images/favicon.ico', true) }}" />
      <link rel="canonical" href="{{ page.url(true, true) }}" />
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
      {% block stylesheets %}
        {% do assets.addCss('theme://css/application.min.css',101) %}
      {% endblock %}
      {{ assets.css() }}
  {% endblock head%}
  </head>
  <body>
    {% block body %}
      {% block content %}{% endblock %}
    {% endblock %}
    {% block javascripts %}
      {% do assets.addJs('theme://js/components/jquery/dist/jquery.min.js') %}
      {% do assets.addJs('theme://js/dist/application.min.js') %}
    {% endblock %}
    {{ assets.js() }}
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-78698997-8', 'auto');
      ga('send', 'pageview');
    </script>
  </body>
</html>
